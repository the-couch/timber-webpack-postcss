{
  "name": "timber-live",
  "version": "1.0.0",
  "description": "The \"_s\" for Timber: a dead-simple theme that you can build from. The primary purpose of this theme is to provide a file structure rather than a framework for markup or styles. Configure your Sass, scripts, and task runners however you would like!",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently 'npm run watch:js' 'npm run watch:css' 'gulp'",
    "build:js": "NODE_ENV=production webpack --progress -p",
    "watch:js": "NODE_ENV=development webpack -w",
    "build:css": "NODE_ENV=production postcss styles/main.css -o assets/main.css",
    "watch:css": "NODE_ENV=development postcss styles/main.css -w -o assets/main.css"
  },
  "author": "iamkevingreen",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.6.0",
    "cssnano": "^3.10.0",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-util": "^3.0.8",
    "lodash-webpack-plugin": "^0.11.5",
    "path": "^0.12.7",
    "postcss": "^6.0.22",
    "postcss-calc": "^6.0.1",
    "postcss-cli": "^5.0.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "standard": "^11.0.1",
    "standard-loader": "^6.0.1",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  }
}
